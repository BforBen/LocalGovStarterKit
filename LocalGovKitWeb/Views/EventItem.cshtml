@inherits Umbraco.Web.Mvc.UmbracoTemplatePage
@{
    Layout = "SideNavPages.cshtml";
}


<div class="page-header">
	<h1>@Umbraco.Field("title")</h1>
</div>
<h3>Starts: @Umbraco.Field("eventStart", formatAsDateWithTime: true, formatAsDateWithTimeSeparator: "")</h3>
<h3>Ends: @Umbraco.Field("eventEnd", formatAsDateWithTime: true, formatAsDateWithTimeSeparator: "")</h3>
@Umbraco.Field("bodyText")