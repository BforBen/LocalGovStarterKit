@inherits Umbraco.Web.Mvc.UmbracoTemplatePage
@{
    Layout = "SiteDesign.cshtml";
}

<div class="row">
    <div class="col-xs-12">
        <div class="page-header">
            <h1>
                @Umbraco.Field("title")
            </h1>
        </div>
        @Umbraco.Field("bodyText")
    </div>
</div>

@Html.Partial( 
    "LandingPageBlock", 
    Model.Content,
    new ViewDataDictionary()
    {
        { "mainCount", @Umbraco.Field("mainItemCount")},
		{ "mainName", "" }
    })